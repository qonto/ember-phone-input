{
  "name": "ember-phone-input",
  "version": "9.0.1",
  "description": "A component to input / validate / submit phone numbers",
  "keywords": [
    "ember-addon",
    "intl-tel-input",
    "international",
    "i18n",
    "country",
    "dial",
    "code",
    "phone",
    "telephone",
    "mobile",
    "input"
  ],
  "homepage": "https://qonto.github.io/ember-phone-input",
  "repository": {
    "type": "git",
    "url": "https://github.com/qonto/ember-phone-input.git"
  },
  "license": "MIT",
  "author": "",
  "files": [
    "addon-main.cjs",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "lint": "concurrently 'pnpm:lint:*(!fix)' --names 'lint:'",
    "lint:fix": "concurrently 'pnpm:lint:*:fix' --names 'fix:'",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js:fix": "eslint . --fix",
    "start": "rollup --config --watch",
    "test": "echo 'A v2 addon does not have tests, run tests in test-app'",
    "prepack": "rollup --config",
    "prepare": "rollup --config"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.0.0",
    "intl-tel-input": "^18.0.0",
    "rsvp": "^4.8.5"
  },
  "devDependencies": {
    "@babel/core": "7.22.9",
    "@babel/eslint-parser": "7.22.9",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-decorators": "7.22.7",
    "@babel/plugin-syntax-decorators": "7.22.5",
    "@embroider/addon-dev": "3.1.1",
    "@rollup/plugin-babel": "6.0.3",
    "concurrently": "8.2.0",
    "ember-template-lint": "5.11.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-ember": "11.10.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.8.8",
    "rollup": "3.26.3",
    "rollup-plugin-copy": "3.4.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs",
    "app-js": {
      "./components/phone-input.js": "./dist/_app_/components/phone-input.js",
      "./instance-initializers/phone-input.js": "./dist/_app_/instance-initializers/phone-input.js",
      "./services/phone-input.js": "./dist/_app_/services/phone-input.js"
    },
    "demoURL": "http://qonto.github.io/ember-phone-input/"
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./addon-main.js": "./addon-main.cjs"
  },
  "volta": {
    "extends": "../package.json"
  }
}
