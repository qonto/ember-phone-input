{
  "name": "ember-phone-input",
  "version": "7.1.1",
  "description": "A component to input / validate / submit phone numbers",
  "keywords": [
    "ember-addon",
    "intl-tel-input",
    "international",
    "i18n",
    "country",
    "dial",
    "code",
    "phone",
    "telephone",
    "mobile",
    "input"
  ],
  "homepage": "https://qonto.github.io/ember-phone-input",
  "repository": {
    "type": "git",
    "url": "https://github.com/qonto/ember-phone-input.git"
  },
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "rollup --config",
    "format:md": "prettier --write **/*.md",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "prepack": "rollup --config",
    "release": "release-it",
    "start": "rollup --config --watch",
    "test": "echo 'A v2 addon does not have tests, run tests in test-app'",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.4",
    "intl-tel-input": "^17.0.13"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.7",
    "@embroider/addon-dev": "^3.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "rollup": "^3.10.0",
    "rollup-plugin-copy": "^3.4.0"
  },
  "resolutions": {
    "ember-cli-addon-docs/ember-svg-jar": "^2.3.3"
  },
  "engines": {
    "node": "12 || >= 14"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "app-js": {},
    "main": "addon-main.js",
    "type": "addon",
    "version": 2
  },
  "volta": {
    "node": "12.22.6",
    "yarn": "1.22.15"
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./addon-main.js": "./addon-main.js"
  },
  "files": [
    "addon-main.js",
    "dist"
  ]
}
