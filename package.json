{
  "name": "ember-phone-input",
  "version": "6.0.0",
  "description": "A component to input / validate / submit phone numbers",
  "keywords": [
    "ember-addon",
    "intl-tel-input",
    "international",
    "i18n",
    "country",
    "dial",
    "code",
    "phone",
    "telephone",
    "mobile",
    "input"
  ],
  "homepage": "https://qonto.github.io/ember-phone-input",
  "repository": {
    "type": "git",
    "url": "https://github.com/qonto/ember-phone-input.git"
  },
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "format:md": "prettier --write **/*.md",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "ember-auto-import": "^1.12.0",
    "ember-cli-babel": "^7.23.0",
    "ember-cli-htmlbars": "^5.7.1",
    "intl-tel-input": "^17.0.13"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.2",
    "@glimmer/tracking": "^1.0.2",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.28.0",
    "ember-cli-addon-docs": "^4.0.0",
    "ember-cli-addon-docs-yuidoc": "^1.0.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.3",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-favicon": "^2.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-release": "^1.0.0-beta.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.1",
    "ember-data": "^3.28.3",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.3",
    "ember-source": "~3.28.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.9.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-plugin-ember": "^10.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "lerna-changelog": "^2.1.0",
    "lint-to-the-future": "^0.6.1",
    "lint-to-the-future-ember-template": "^0.3.5",
    "lint-to-the-future-eslint": "^0.2.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "qunit-dom": "^2.0.0"
  },
  "resolutions": {
    "ember-cli-addon-docs/ember-auto-import": "^1.12.0",
    "ember-cli-addon-docs/ember-svg-jar": "^2.3.3",
    "ember-cli-addon-docs/webpack": "^4.46.0"
  },
  "engines": {
    "node": "12 || >= 14"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://qonto.github.io/ember-phone-input/",
    "versionCompatibility": {
      "ember": ">3.3.0"
    }
  },
  "volta": {
    "node": "12.22.6",
    "yarn": "1.22.15"
  }
